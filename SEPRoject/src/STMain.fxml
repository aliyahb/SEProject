<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="STMainController">
   <children>
      <Pane fx:id="searchPane" layoutX="145.0" prefHeight="500.0" prefWidth="555.0" />
      <Pane fx:id="viewPane" layoutX="145.0" prefHeight="507.0" prefWidth="555.0" style="-fx-border-color: grey;">
         <children>
            <TableView fx:id="table" layoutY="81.0" prefHeight="368.0" prefWidth="555.0">
              <columns>
                <TableColumn fx:id="idCol" editable="false" prefWidth="65.0" resizable="false" text="ID" />
                <TableColumn fx:id="buildCol" editable="false" prefWidth="95.0" text="Building" />
                  <TableColumn fx:id="roomCol" editable="false" prefWidth="75.0" text="Room" />
                  <TableColumn fx:id="dateCol" prefWidth="119.0" text="Date" />
                  <TableColumn fx:id="sTimeCol" editable="false" prefWidth="100.0" text="Start Time" />
                  <TableColumn fx:id="eTimeCol" editable="false" prefWidth="100.0" text="End Time" />
              </columns>
            </TableView>
            <Button fx:id="cancel" layoutX="401.0" layoutY="458.0" mnemonicParsing="false" onAction="#cancelBooking" prefHeight="35.0" prefWidth="140.0" style="-fx-background-color: white;" text="Cancel Booking" textAlignment="CENTER" />
            <Text layoutX="12.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="My Bookings:" wrappingWidth="145.0">
               <font>
                  <Font name="Arial" size="21.0" />
               </font>
            </Text>
         </children>
      </Pane>
      <Pane prefHeight="507.0" prefWidth="145.0" style="-fx-border-color: grey;">
         <children>
            <Label fx:id="nameLabel" alignment="CENTER" layoutY="14.0" prefHeight="35.0" prefWidth="145.0" text="Name" textAlignment="CENTER">
               <font>
                  <Font size="19.0" />
               </font></Label>
            <Label fx:id="typeLabel" alignment="CENTER" layoutX="1.0" layoutY="58.0" prefHeight="35.0" prefWidth="145.0" text="accType" textAlignment="CENTER" />
            <Button fx:id="logout11" layoutX="3.0" layoutY="209.0" mnemonicParsing="false" onAction="#searchRooms" prefHeight="35.0" prefWidth="140.0" style="-fx-background-color: white;" text="Search Rooms" textAlignment="CENTER" />
            <Button fx:id="logout1" layoutX="3.0" layoutY="254.0" mnemonicParsing="false" onAction="#viewBookings" onMouseClicked="#viewBookings" prefHeight="35.0" prefWidth="140.0" style="-fx-background-color: white;" text="My Bookings" textAlignment="CENTER" />
            <Button fx:id="logout" layoutX="3.0" layoutY="435.0" mnemonicParsing="false" onAction="#Logout" prefHeight="35.0" prefWidth="140.0" style="-fx-background-color: white;" text="Logout" textAlignment="CENTER" />
            <Label fx:id="allowLabel" alignment="CENTER" layoutX="1.0" layoutY="151.0" prefHeight="35.0" prefWidth="145.0" text="Allowance:" textAlignment="CENTER" />
            <Button fx:id="logout12" layoutX="4.0" layoutY="303.0" mnemonicParsing="false" onMouseClicked="#viewBookings" prefHeight="35.0" prefWidth="140.0" style="-fx-background-color: white;" text="Requests" textAlignment="CENTER" />
         </children>
      </Pane>
   </children>
</AnchorPane>
